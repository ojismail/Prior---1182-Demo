<!--Footer Script -->
<script>
  // Get the modal element
  var modal = document.querySelector('.modal');

  // Get the link that opens the modal
  var contactLink = document.querySelector('.contact-link');

  // Get the close button element
  var closeButton = document.querySelector('.close');

  // Add click event listener to contact link
  contactLink.addEventListener('click', function() {
    modal.style.display = 'block';
  });

  // Add click event listener to close button
  closeButton.addEventListener('click', function() {
    modal.style.display = 'none';
  });

  // Add click event listener to modal background
  window.addEventListener('click', function(event) {
    if (event.target == modal) {
      modal.style.display = 'none';
    }
  });

  // Get the data consent modal
  var dataModal = document.getElementById("myModal");

  // Get the accept button
  var acceptBtn = document.getElementById("acceptBtn");

  // When the page loads, show the data consent modal if the user has not accepted the terms
  if (!localStorage.getItem("consent")) {
    dataModal.style.display = "block";
  }

  // When the user clicks the accept button, store their consent and hide the data consent modal
  acceptBtn.onclick = function() {
    localStorage.setItem("consent", true);
    dataModal.style.display = "none";
  }
</script>
